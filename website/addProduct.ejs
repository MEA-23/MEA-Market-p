<!DOCTYPE html>
<html>
<%-include('./blocks/head')%>
<body>

    <%-include('./blocks/nav')%>



    <div class="container ">
        <div class="row justify-content-center">
          <div class="col-md-6">
            <form id="productForm">
              <div class="mb-3">
                <label for="product_name" class="form-label">Product Name</label>
                <input type="text" class="form-control product_name" required>
              </div>
              <div class="mb-3">
                <label for="product_price" class="form-label">Product Price</label>
                <input type="number" class="form-control product_price" required>
              </div>
              <div class="mb-3">
                <label for="product_image" class="form-label">Product Image URL</label>
                <input type="text" class="form-control product_image" required>
              </div>
              <div class="mb-3">
                <label for="product_description" class="form-label">Product Description</label>
                <textarea class="form-control product_description" rows="4" required></textarea>
              </div>
              <div class="mb-3">
                <label for="product_category" class="form-label">Product Category</label>
                <input type="text" class="form-control product_category" required>
              </div>
              <div class="mb-3">
                <label for="product_quantity" class="form-label">Product Quantity</label>
                <input type="number" class="form-control product_quantity" required>
              </div>
              <button type="submit" class="btn btn-primary">Submit</button>
            </form>
          </div>
        </div>
      </div>
<br>.
<br>
<br>


<script src="/script/createProductClint.js"></script>
</body>
</html>